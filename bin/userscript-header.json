{
    "name": "%DISPLAYNAME%",
    "namespace": "%NAMESPACE%",
    "version": "%VERSION%",
    "description": "%DESCRIPTION%",
    "author": "%AUTHOR%",
    "match": [
        "https://e621.net/*",
        "https://e926.net/*"
    ],
    "updateURL": "%GITHUB%/releases/latest/download/script.user.js",
    "downloadURL": "%GITHUB%/releases/latest/download/script.user.js",
    "require": [
        "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/jszip/3.3.0/jszip.min.js",
        "https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/1.3.8/FileSaver.min.js",
        "https://cdn.jsdelivr.net/gh/re621/re621-lib/lazysizes/lazysizes.min.js",
        "https://cdn.jsdelivr.net/npm/jquery.hotkeys@0.1.0/jquery.hotkeys.min.js"
    ],
    "resource": {
        "re621_styles": "%GITHUB%/releases/download/%VERSION%/style.min.css",
        "re621_dnp": "%GITHUB%/releases/download/%VERSION%/avoid-posting.json"
    },
    "grant": [
        "GM.setValue",
        "GM.getValue",
        "GM.deleteValue",
        "GM.openInTab",
        "GM.setClipboard",

        "GM.getResourceUrl",
        "GM_getResourceURL",
        "GM.xmlhttpRequest",
        "GM_xmlhttpRequest",

        "unsafeWindow"
    ],
    "icon64": "https://re621.github.io/images/icon.png",
    "connect": [
        "api.github.com",
        "static1.e621.net"
    ],
    "run-at": "document-start"
}
