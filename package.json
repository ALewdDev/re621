{
    "name": "re621",
    "displayName": "re621 - e621 Reimagined",
    "namespace": "re621.github.io",
    "version": "1.1.0",
    "description": "Increases the amount of customization on e621",
    "author": "bitWolfy",
    "main": "./build/script.user.js",
    "scripts": {
        "autoprefixer": "postcss ./build/style.min.css --no-map --use autoprefixer -r",
        "build:prod": "npm-run-all eslint typescript browserify minify userscript creatednp sass autoprefixer clean",
        "build:dev": "npm-run-all eslint typescript browserify userscript sass autoprefixer clean",
        "eslint": "eslint ./**/*.ts",
        "clean": "rimraf ./build/temp/",
        "test": "echo \"Error: no test specified\" && exit 1",
        "typescript": "tsc",
        "browserify": "browserify ./build/temp/js/main.js -o ./build/script.user.js",
        "minify": "terser ./build/script.user.js --compress -o ./build/script.user.js",
        "userscript": "node ./bin/userscript-format.js",
        "sass": "sass ./src/scss/style.scss ./build/style.min.css --no-source-map --style compressed",
        "creatednp": "node ./bin/create-dnp.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/re621/re621.git"
    },
    "license": "GPLv3",
    "bugs": {
        "url": "https://github.com/re621/re621/issues"
    },
    "homepage": "https://re621.github.io/",
    "dependencies": {
        "jquery": "^3.4.1",
        "jquery-ui": "^1.12.1"
    },
    "devDependencies": {
        "@actions/core": "^1.2.3",
        "@types/jquery": "^3.3.33",
        "@types/jqueryui": "^1.12.10",
        "@typescript-eslint/eslint-plugin": "^2.25.0",
        "@typescript-eslint/parser": "^2.25.0",
        "autoprefixer": "^9.7.5",
        "browserify": "^16.5.0",
        "eslint": "^6.8.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.0",
        "rimraf": "^3.0.2",
        "sass": "^1.26.3",
        "terser": "^4.6.10",
        "typescript": "^3.8.3"
    }
}
